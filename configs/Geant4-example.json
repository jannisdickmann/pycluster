{
    "cluster": {
        "partition": {"default": "local"},
        "max_memory": "2G",
        "max_time": "1-00:00:00",
        "mail": "ALL"
    },
    "executable": "/path/to/Genat4executable",
    "output_directory": "~/output/simulation",
    "project_name": "{{ identifier }}-{{ phantom }}@{{ energy }}MeV",
    "array": {
        "first": 0,
